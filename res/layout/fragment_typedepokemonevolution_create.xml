<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:harmony="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <TextView
        android:id="@+id/typedepokemonevolution_evolueen_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/typedepokemonevolution_evolueen_label"/>
    <com.maximeleau.harmony.android.pokemon.harmony.widget.SingleEntityWidget
        android:id="@+id/typedepokemonevolution_evolueen_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/typedepokemonevolution_evolueen_label" />
    <TextView
        android:id="@+id/typedepokemonevolution_estevolueen_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/typedepokemonevolution_evolueen_button"
        android:text="@string/typedepokemonevolution_estevolueen_label"/>
    <com.maximeleau.harmony.android.pokemon.harmony.widget.SingleEntityWidget
        android:id="@+id/typedepokemonevolution_estevolueen_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/typedepokemonevolution_estevolueen_label" />
</RelativeLayout>
